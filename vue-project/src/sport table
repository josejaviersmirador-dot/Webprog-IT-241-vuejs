<script setup>
import { ref, onMounted } from 'vue'
import { supabase } from './lib/supabaseClient'

const sports = ref([])

async function getSports() {
  const { data } = await supabase.from('Sport').select()
  sports.value = data
}

onMounted(() => {
  getSports()
})
</script>

<template>
  <div>
    <h1>Sports from Supabase</h1>
    <ul>
      <li v-for="sport in sports" :key="sport.id">
        {{ sport.Name }}
      </li>
    </ul>
  </div>
</template>
